<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">
	<managed-bean>
		<managed-bean-name>
		loginHandler</managed-bean-name>
		<managed-bean-class>
		br.com.shadow.handler.LoginHandler</managed-bean-class>
		<managed-bean-scope>
		session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>
		crudAcaoHandler</managed-bean-name>
		<managed-bean-class>
		br.com.shadow.handler.CrudAcaoHandler</managed-bean-class>
		<managed-bean-scope>
		session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>
		flotHandler</managed-bean-name>
		<managed-bean-class>
		br.com.shadow.handler.FlotHandler</managed-bean-class>
		<managed-bean-scope>
		session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>
		crudOpcaoHandler</managed-bean-name>
		<managed-bean-class>
		br.com.shadow.handler.CrudOpcaoHandler</managed-bean-class>
		<managed-bean-scope>
		session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>
		crudSerieOpcaoHandler</managed-bean-name>
		<managed-bean-class>
		br.com.shadow.handler.CrudSerieOpcaoHandler</managed-bean-class>
		<managed-bean-scope>
		session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>
		crudAdminHandler</managed-bean-name>
		<managed-bean-class>
		br.com.shadow.handler.CrudAdminHandler</managed-bean-class>
		<managed-bean-scope>
		session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>
		cockpitHandler</managed-bean-name>
		<managed-bean-class>
		br.com.shadow.handler.CockpitHandler</managed-bean-class>
		<managed-bean-scope>
		session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>
		dtaHandler</managed-bean-name>
		<managed-bean-class>
		br.com.shadow.handler.DetalheTravaAltaHandler</managed-bean-class>
		<managed-bean-scope>
		session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>
		negociosHandler</managed-bean-name>
		<managed-bean-class>
		br.com.shadow.handler.NegociosHandler</managed-bean-class>
		<managed-bean-scope>
		session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>
		analiseHandler</managed-bean-name>
		<managed-bean-class>
		br.com.shadow.handler.AnaliseHandler</managed-bean-class>
		<managed-bean-scope>
		session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>
		negociosRoboHandler</managed-bean-name>
		<managed-bean-class>
		br.com.shadow.handler.NegociosRoboHandler</managed-bean-class>
		<managed-bean-scope>
		session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>
		logout</display-name>
		<from-view-id>
		/logout.jsp</from-view-id>
		<navigation-case>
			<from-outcome>
			ok</from-outcome>
			<to-view-id>
			/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>
		login</display-name>
		<from-view-id>
		/login.jsp</from-view-id>
		<navigation-case>
			<from-outcome>
			logged</from-outcome>
			<to-view-id>
			/top.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>

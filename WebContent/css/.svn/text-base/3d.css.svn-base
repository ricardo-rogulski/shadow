
.tooltip {
  display:inline;
  font-size:13px;
  font-family:tahoma, sans-serif; 
  color:black;
  background-color:#EEEEEE;                     
  position:absolute;     
  z-order:1; 
  padding:0px 5px 3px 5px;
  border:1px solid black;
}

.pad {   
  font-family:verdana; 
  font-size:11px; 
  text-align:center;
  background-color:#f4f0D8;
  cursor:hand;
  padding:5px 5px 3px 5px;
}

.hpad {
  font-family:verdana; 
  font-size:11px; 
  text-align:center;
  font-weight:bold;
  background-color:#f4f0D8;
  cursor:hand;
  padding:5px;
}

.padtable {
  background-color:#f4f0D8;
  display:none;
  width:100%; 
  height:100%;
  padding:5px;
}
.collapsebtn {
  cursor:hand; 
  font-family:courier; 
  font-size:11px; 
  border:1px solid black; 
  padding:0px 3px 0px 3px;
}

.item {
  color:blue;
  padding:1px 5px 1px 5px;
  cursor:hand; 
  width:100%;
}

.hitem {
  background-color:#d4d0b8;
  color:blue;
  text-decoration:underline;
  font-weight:bold;
  padding:1px 5px 1px 5px;
  cursor:hand; 
}

.zone {
  border: 2px solid #f4f0D8;
}

.hzone {
  border: 2px solid #778899;
}

#doc_tree {
	height:100%;
	width:100%;
	overflow-x:visible; overflow-y:auto;
	text-align:left;
}
#doc_location {
	height:100%;
	width:100%;
	overflow-x:visible; overflow-y:auto;
	text-align:left;
}
#padtable_tree {
	text-align:left;
	height:100%;
	width:100%;
}
#zones_list {
	height:100%;
	width:100%;
	overflow-x:visible; overflow-y:auto;
	text-align:left;
}
#padtable_zones {
	text-align:left;
	height:100%;
	width:100%;
}
#structures_list {
	height:100%;
	width:100%;
	overflow-x:visible; overflow-y:auto;
	text-align:left;
}
.doclink {
  color:blue;
  padding:1px 5px 1px 5px;
  cursor:hand; 
  text-decoration : none;
}
.hdoclink{
  text-decoration:underline;
  color:blue;
  padding:1px 5px 1px 5px;
  cursor:hand; 
}